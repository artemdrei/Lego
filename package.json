{
  "name": "react-lego",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --watch",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "eslint": "eslint '*/**/*.{js,ts,tsx}'",
    "tslint:watch": "tsc --skipLibCheck --allowJs --watch",
    "test": "jest",
    "test:watch": "jest --watch --verbose false",
    "stats": "webpack --json > stats.json && webpack-bundle-analyzer stats.json",
    "svg:optimize": "find ./src/assets/svg -type f -name '*.svg' -print0 | xargs -0 -n 1 -P 6 svgo",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "browserslist": "> 1%, ie >= 11, not dead",
